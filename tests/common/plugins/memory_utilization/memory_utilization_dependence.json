{
    "HWSKU" : {
      "Arista-7050QX": ["Arista-7050-QX-32S", "Arista-7050-QX32", "Arista-7050QX-32S-S4Q31", "Arista-7050QX32S-Q32"]
    },
    "COMMON": [
      {
        "name": "top",
        "cmd": "top -b -n 1",
        "memory_params": {
          "bgpd": {
            "memory_increase_threshold": 81920,
            "memory_high_threshold": 307200
          },
          "zebra": {
            "memory_increase_threshold": 51200,
            "memory_high_threshold": 204800
          }
        },
        "memory_check": "parse_top_output"
      },
      {
        "name": "free",
        "cmd": "free -m",
        "memory_params": {
          "used": {
            "memory_increase_threshold": 512,
            "memory_high_threshold": 3072
          }
        },
        "memory_check": "parse_free_output"
      },
      {
        "name": "monit",
        "cmd": "sudo monit status",
        "memory_params": {
          "memory_usage": {
            "memory_increase_threshold": 5,
            "memory_high_threshold": 65
          }
        },
        "memory_check": "parse_monit_status_output"
      },
      {
        "name": "docker",
        "cmd": "docker stats --no-stream",
        "memory_params": {
          "snmp": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          },
          "pmon": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 10
          },
          "lldp": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          },
          "gnmi": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          },
          "radv": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          },
          "syncd": {
            "memory_increase_threshold": 5,
            "memory_high_threshold": 20
          },
          "bgp": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 10
          },
          "teamd": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          },
          "swss": {
            "memory_increase_threshold": 5,
            "memory_high_threshold": 15
          },
          "acms": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          },
          "database": {
            "memory_increase_threshold": 2,
            "memory_high_threshold": 5
          }
        },
        "memory_check": "parse_docker_stats_output"
      }
    ],
    "Arista-7050QX": [
      {
        "name": "monit",
        "cmd": "sudo monit status",
        "memory_params": {
          "memory_usage": {
            "memory_increase_threshold": 5,
            "memory_high_threshold": 85
          }
        },
        "memory_check": "parse_monit_status_output"
      }
    ]
}
